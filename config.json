{
  "name": "khalista",
  "version": "1.2.0",
  "description": "A modern web application template",
  "settings": {
    "theme": "dark",
    "language": "en",
    "debug": false,
    "cache": true,
    "analytics": true,
    "notifications": {
      "email": true,
      "push": true,
      "sms": false
    },
    "security": {
      "twoFactor": true,
      "passwordPolicy": "strong",
      "rateLimit": true
    },
    "performance": {
      "compression": true,
      "minification": true,
      "lazyLoading": true,
      "caching": {
        "browser": true,
        "server": true,
        "duration": 3600
      }
    }
  }
} 