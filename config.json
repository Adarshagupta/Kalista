{
  "name": "khalista",
  "version": "1.3.0",
  "description": "A modern web application template",
  "settings": {
    "theme": "auto",
    "language": "en",
    "debug": false,
    "cache": true,
    "analytics": true,
    "notifications": {
      "email": true,
      "push": true,
      "sms": true
    },
    "security": {
      "twoFactor": true,
      "passwordPolicy": "strong",
      "rateLimit": true
    },
    "performance": {
      "compression": true,
      "minification": true,
      "lazyLoading": true,
      "caching": {
        "browser": true,
        "server": true,
        "duration": 3600
      }
    },
    "i18n": {
      "defaultLocale": "en",
      "supportedLocales": ["en", "es", "fr", "de", "ja"],
      "fallbackLocale": "en",
      "autoDetect": true,
      "dateFormat": "MM/DD/YYYY",
      "timeFormat": "24h"
    }
  }
} 
